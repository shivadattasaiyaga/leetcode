class Solution {
    public static int floor(Node root, int x) {
        // Code here
        if (root == null) return -1;
        int floor=-1;
        while(root!=null){
            if(x==root.data){
                return x;
            }
            else if(x<root.data){
                root=root.left;
            }
            else if(x>root.data){
                floor=root.data;
                root=root.right;
            }
        }
        return floor;
    }
}
